#!/bin/bash
DIR=`dirname $0`

declare -i start_with=`date +%s`
time ${DIR}/subscribe $*
declare -i end_with=`date +%s`

echo time with subscriber on = $end_with - $start_with "(seconds)"
  
declare -i start_without=`date +%s`
$*
declare -i end_without=`date +%s`

echo time with subscriber off = $end_without - $start_without "(seconds)"

